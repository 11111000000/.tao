set autocomplete=0
colorscheme zenburn
set scroll=10
set guioptions-=m
set guioptions+=T
set guioptions+=s
"set tabopen=link:right,orphan:end,external:end

map <C-W> <Esc>
map <C-Q> <Esc>
map <A-f> <F11>
map <S-J> <C-d>
map <S-K> <C-u>
map <C-j> <Esc>
map <C-k> <Esc>
map <C-h> <C-PageUp>
map <C-l> <C-PageDown>
map <A-C-h> :tabprev<CR>
map < gT 
map > gt 
map <A-C-l> :tabnext<CR>
imap <A-C-h> <Esc>:tabprev<CR>
imap <A-C-l> <Esc>:tabnext<CR>

imap <C-j> <Esc><C-d>
imap <C-k> <Esc><C-u>
imap <C-h> <C-PageUp>
imap <C-l> <C-PageDown>

map <A-j> :wincmd j<CR>
map <A-k> :wincmd k<CR>
map <A-h> :wincmd h<CR>
map <A-l> :wincmd l<CR>

map <A-a> :firebug console-focus<CR>
map <A-c> :firebug console-clear<CR>

map d <Esc>
map <A-d> :tabclose<CR>

map <A-b> :dialog bookmarks<CR>
map <A-r> :dialog cleardata<CR>
map <C-Space> :
map <Space> :

